// CHECK_ENV_PATH (Server handler)
// CHECK_ENV_PATH_COMPLETE (UI handler)
// SET_ENV_PATH => Register environment variable in the System (Server handler)
// SET_ENV_PATH_COMPLETE => Register environment variable in the System (UI handler) => ENV_PATH_VARIABLE (String), Path (String)
// INSTALL_COMP => (Server handler)
// CHECK_PREREQUISITES => (Server handler)
// CHECK_PREREQUISITES_COMPLETE => (UI handler)
// DOWNLOAD_FILE_COMPLETE (UI handler)
// EXTRACT_FILE_COMPLETE (UI handler)
// REGISTER_COMP_COMPLETE (UI handler)
// CREATE_SHORTCUTS_COMPLETE (UI handler)
// INSTALL_COMP_COMPLETE (UI handler)
// ERROR_PACKAGE_MANAGER (UI handler)
{
    "command": "CHECK_ENV_PATH",
    "data": {
        "envVarName": "LG_WEBOS_STUDIO_SDK_HOME"
    }
}
{
    "command": "CHECK_ENV_PATH_COMPLETE",
    "data": {
        "envVarName": "LG_WEBOS_STUDIO_SDK_HOME",
        "envVarValue": ""
    }
}
{
    "command": "SET_ENV_PATH",
    "data": {
        "envVarName": "LG_WEBOS_STUDIO_SDK_HOME",
        "envVarValue": "C:/abcd"
    }
}
{
    "command": "SET_ENV_PATH_COMPLETE",
    "data": {
        "envVarName": "LG_WEBOS_STUDIO_SDK_HOME",
        "envVarValue": "C:/abcd",
        "isSet": true,
        "errMsg": ""
    }
}
{
    "command": "CHECK_PREREQUISITES",
    "data": {
        "sdk": "tv",
        "component": "cli",
        "componentInfo": {
            "sdk_version": "1.0.1",
            "apiLevel": "21",
            "comp_uid": "tv_cli_1",
        },
    }
}
{
    "command": "CHECK_PREREQUISITES_COMPLETE",
    "data": {
        "sdk": "tv",
        "component": "cli",
        "componentInfo": {
            "comp_uid": "tv_cli_1",
            },
        "isSuccess": true,
        "message": ""
    }
}

{
    "command": "INSTALL_COMP",
    "data": {
        "sdk": "tv",
        "sdkSubDirName": "tv",
        "component": "cli",
        "componentSubDirName": "cli",
        "componentInfo": {
            "comp_uid": "tv_cli_1",
